<tool id="pdf2htmlEX" name="pdf2htmlEX">
    <description>pdf to html converter</description>
    <requirements>
	<container type="docker">bwits/pdf2htmlex</container>
    </requirements>
    <command>
 <![CDATA[
	
 pdf2htmlEX $input pdfhtml.html 2>log

  ]]> </command>
    <inputs>
	<param format="pdf" name="input" label="input pdf file" type="data" />
    </inputs>
    <outputs>
        <data name="output" format="html" from_work_dir="pdfhtml.html" label="${tool.name} on ${on_string} :html"/>
    </outputs>
    <help>
insert pdf2htmlEX in the withelist in order to see the result using galaxy
    </help>
</tool>
